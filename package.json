{
  "name": "balance-raiting",
  "version": "1.0.0",
  "description": "",
  "module": "app/index.js",
  "type": "module",
  "keywords": [
    "async",
    "fastQ",
    "bull",
    "rabbibMQ",
    "docker"
  ],
  "author": "GRD",
  "license": "ISC",
  "scripts": {
    "start:prod": "npm run build && NODE_ENV=production node app/index.js",
    "start:dev": "tsc-watch --onSuccess \"npm run watch\"",
    "watch": "nodemon --watch '_src/**/*.ts' --exec \"NODE_ENV=development node --experimental-specifier-resolution=node --loader ts-node/esm\" _src/index.ts",
    "run-typescript": "ts-node-transpile-only",
    "build": "tsc && node app/utils/fix-imports.util.js",
    "test": "jest && NODE_ENV=test",
    "test:watch": "jest --watch && NODE_ENV=test",
    "test:cov": "jest --coverage && NODE_ENV=test",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand && NODE_ENV=test",
    "test:e2e": "jest --config ./test/jest-e2e.json && NODE_ENV=test"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "body-parser": "^1.19.0",
    "bull": "^4.10.4",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "fastq": "^1.15.0",
    "node-fetch": "^3.3.1",
    "reflect-metadata": "^0.1.13",
    "toad-scheduler": "^3.0.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.7",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2"
  }
}
